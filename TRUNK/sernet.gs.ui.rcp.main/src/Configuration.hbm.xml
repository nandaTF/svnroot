<hibernate-mapping package="sernet.gs.ui.rcp.main.common.model.configuration">
	<class name="Configuration">
		
		<id name="dbId" 
			type="java.lang.Integer" 
			column="dbId"
			unsaved-value="null">
			<generator class="native" />
		</id>
		
		<many-to-one 
			name="entity"
			column="entity_id"
			class="sernet.hui.common.connect.Entity"
			cascade="all"
			fetch="join"
		/>
		
		<many-to-one 
			name="person"
			column="person_id"
			class="sernet.gs.ui.rcp.main.bsi.model.Person"	
		/>

		
		
	</class>
</hibernate-mapping>
