<hibernate-mapping package="sernet.gs.ui.rcp.main.bsi.risikoanalyse.model">
	<class name="FinishedRiskAnalysisLists">
		
		<id name="dbId" 
			type="java.lang.Integer" 
			column="dbId"
			unsaved-value="null">
			<generator class="native" />
		</id>
		
		<property
			name="uuid"
			unique="true"
		/>
		
		<property name="finishedRiskAnalysisId" />
		
		<list name="allGefaehrdungsUmsetzungen" cascade="all-delete-orphan">
            <key column="riskanalysislists_id1" />
            <index column="idx1"/>
            <many-to-many class="GefaehrdungsUmsetzung"/>
        </list>

		<list name="associatedGefaehrdungen" cascade="all-delete-orphan">
            <key column="riskanalysislists_id2" />
            <index column="idx2"/>
            <many-to-many class="GefaehrdungsUmsetzung"/>
        </list>

		<list name="notOKGefaehrdungsUmsetzungen" cascade="all-delete-orphan">
            <key column="riskanalysislists_id3" />
            <index column="idx3"/>
            <many-to-many class="GefaehrdungsUmsetzung"/>
        </list>
		
		
	</class>
</hibernate-mapping>
