<hibernate-mapping package="sernet.gs.ui.rcp.main.common.model">
	<class name="CnALink"
		mutable="false">
		
		<composite-id name="id" class="CnALink$Id">
			<key-property name="dependantId"
				access="field"
				column="DEPENDANT_ID"/>
			<key-property name="dependencyId"
				access="field"
				column="DEPENDENCY_ID"/>
		</composite-id>
		
		<property name="linkType"/>
		
		<many-to-one name="dependant"
			column="DEPENDANT_ID"
			not-null="true"
			insert="false"
			update="false"/>

		<many-to-one name="dependency"
			column="DEPENDENCY_ID"
			not-null="true"
			insert="false"
			update="false"
			fetch="join"/>
		
	</class>
</hibernate-mapping>
