<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="sernet.gs.ui.rcp.main.common.model">
	<class name="CnATreeElement" 
		batch-size="10"
		abstract="true">
		<id name="dbId" 
			type="java.lang.Integer" 
			column="dbId"
			unsaved-value="null">
			<generator class="native" />
		</id>
		
		<discriminator column="OBJECT_TYPE" type="string"/>	
		
		<property
			name="uuid"
		/>
		
		<set name="permissions"
			cascade="all"
			inverse="true"
			batch-size="10">
			<key column="cte_id"/>
			<one-to-many class="Permission"/>
		</set>
		
		<many-to-one 
			name="entity"
			column="entity_id"
			class="sernet.hui.common.connect.Entity"
			cascade="all"
			fetch="join"
			/>
			
		<many-to-one 
			name="parent"
			column="parent"
			class="CnATreeElement" 	
			/>
			
		<set name="children" 
			cascade="all"
			inverse="true"
			batch-size="10"
			>
			<key column="parent" />
			<one-to-many class="CnATreeElement"/>
		</set>
		
		<set name="linksDown"
			inverse="true"
			cascade="delete,delete-orphan">
			<key column="DEPENDANT_ID"/>
			<one-to-many class="CnALink"/>
		</set>
		
		<set name="linksUp"
			inverse="true"
			cascade="delete,delete-orphan">
			<key column="DEPENDENCY_ID"/>
			<one-to-many class="CnALink"/>
		</set>
		
		
<!-- Grundschutz-Elemente: ################################################################# -->

