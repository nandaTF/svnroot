            id="sernet.gs.ui.rcp.main.page2"
            name="Datenbank"/>
      <page
            class="sernet.gs.ui.rcp.main.preferences.ReportPreferencePage"
            id="sernet.gs.ui.rcp.main.page3"
            name="Reporting / Export"/>
      <page
            class="sernet.gs.ui.rcp.main.preferences.GeneralSettingsPage"
            id="sernet.gs.ui.rcp.main.page4"
            name="Allgemeine Einstellungen"/>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer class="sernet.gs.ui.rcp.main.preferences.PreferenceInitializer"/>
   </extension>
   <extension
         point="org.eclipse.ui.cheatsheets.cheatSheetContent">
      <category
            id="sernet.gs.ui.rcp.main.cheatSheets"
            name="verinice Tutorials"/>
      <cheatsheet
            category="sernet.gs.ui.rcp.main.cheatSheets"
            composite="false"
            contentFile="cheatsheets/ErsteSchritte.xml"
            id="sernet.gs.ui.rcp.main.cheatsheet1"
            name="1_Erste Schritte">
         <description>
         Dieser Spickzettel erklaert die ersten Schritte mit verinice.
         </description>
      </cheatsheet>
      <cheatsheet
            category="sernet.gs.ui.rcp.main.cheatSheets"
            composite="false"
            contentFile="cheatsheets/Strukturanalse.xml"
            id="sernet.gs.ui.rcp.main.cheatsheet2"
            name="2_Strukturanalyse">
            <description>
            So erfassen Sie mit verinice Ihren IT-Verbund.
            </description>
        </cheatsheet>
      <cheatsheet
            category="sernet.gs.ui.rcp.main.cheatSheets"
            composite="false"
            contentFile="cheatsheets/Schutzbedarfsanalyse.xml"
            id="sernet.gs.ui.rcp.main.schutzbedarf"
            name="3_Schutzbedarfsanalyse">
            <description>
            Schutzbedarf zuweisen und Objekte verknüpfen.
            </description>
        </cheatsheet>
      <cheatsheet
            category="sernet.gs.ui.rcp.main.cheatSheets"
            composite="false"
            contentFile="cheatsheets/Modellierung.xml"
            id="sernet.gs.ui.rcp.main.modellierung"
            name="4_Modellierung nach IT-Grundschutz">
            <description>
            Zuordnen von Bausteinen zu Zielobjekten.
            </description>
        </cheatsheet>
        <cheatsheet
            category="sernet.gs.ui.rcp.main.cheatSheets"
            composite="false"
            contentFile="cheatsheets/BasisSicherheitscheck.xml"
            id="sernet.gs.ui.rcp.main.basischeck"
            name="5_Basis-Sicherheitscheck">
            <description>
            Maßnahmen dokumentieren, einfach und mit Hilfsmitteln.
            </description>
        </cheatsheet>
        <cheatsheet
            category="sernet.gs.ui.rcp.main.cheatSheets"
            composite="false"
            contentFile="cheatsheets/ErgaenzendeAnalyse.xml"
            id="sernet.gs.ui.rcp.main.ergaenzendeanalyse"
            name="6_Ergänzende Sicherheitsanalyse">
            <description>
            So dokumentieren Sie die Durchführung der ergänzenden Sicherheitsanalyse.
            </description>
        </cheatsheet>
        <cheatsheet
            category="sernet.gs.ui.rcp.main.cheatSheets"
            composite="false"
            contentFile="cheatsheets/Risikoanalyse.xml"
            id="sernet.gs.ui.rcp.main.risikoanalyse"
            name="7_Risikoanalyse">
            <description>
            Risikoanalyse mit verinice.
            </description>
        </cheatsheet>
        <cheatsheet
            category="sernet.gs.ui.rcp.main.cheatSheets"
            composite="false"
            contentFile="cheatsheets/Reports.xml"
            id="sernet.gs.ui.rcp.main.reports"
            name="8_Erzeugen von Reports">
            <description>
            So erzeugen Sie die vom BSI geforderten Referenzdokumente und weitere Reports.
            </description>
        </cheatsheet>
        <cheatsheet
            category="sernet.gs.ui.rcp.main.cheatSheets"
            composite="false"
            contentFile="cheatsheets/Datenschutz.xml"
            id="sernet.gs.ui.rcp.main.datenschutz"
            name="9_Datenschutz">
            <description>
               Verfahren für Datenschutz dokumentieren.
            
            </description>
        </cheatsheet>
