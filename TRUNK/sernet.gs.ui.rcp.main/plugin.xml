            icon="icons/de.png"
            id="sernet.gs.ui.rcp.main.views.bsimassnahmenview"
            name="IT-Grundschutz">
      </view>
      <category
            id="sernet.gs.ui.rcp.main.views"
            name="Grundschutz"/>
      <view
            allowMultiple="false"
            category="sernet.gs.ui.rcp.main.views"
