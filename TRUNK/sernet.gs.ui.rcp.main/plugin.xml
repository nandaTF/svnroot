            icon="icons/shield.png"
            id="sernet.gs.ui.rcp.main.views.dsmodelview"
            name="Datenschutz"/>
      <view
            allowMultiple="false"
            category="sernet.gs.ui.rcp.main.views"
            class="sernet.gs.ui.rcp.main.bsi.views.TodoView"
