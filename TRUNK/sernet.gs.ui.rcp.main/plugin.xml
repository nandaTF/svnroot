<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="sernet.gs.ui.rcp.main.Application">
         </run>
      </application>
   </extension>
   <extension
