            class="sernet.gs.ui.rcp.main.Perspective"
            icon="icons/de.png"
            id="sernet.gs.ui.rcp.main.perspective"
            name="Datensicherheit">
      </perspective>
      <perspective
            class="sernet.gs.ui.rcp.main.PerspectiveDS"
            icon="icons/sample.gif"
            id="sernet.gs.ui.rcp.main.dsperspective"
            name="Datenschutz"/>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            allowMultiple="true"
            category="sernet.gs.ui.rcp.main.views"
            class="sernet.gs.ui.rcp.main.bsi.views.BrowserView"
            icon="icons/tag.png"
            id="sernet.gs.ui.rcp.main.bsi.views.browserview"
            name="Maßnahmen-Browser">
      </view>
      <view
            allowMultiple="false"
            category="sernet.gs.ui.rcp.main.views"
