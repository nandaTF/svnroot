      </handler>
      <handler
            class="sernet.verinice.iso27k.rcp.action.PasteHandler"
            commandId="org.eclipse.ui.edit.paste">
         <activeWhen>
         	<with variable="activePartId">
     			<or>
       				<equals value="sernet.verinice.iso27k.rcp.ISMView" />
       				<equals value="sernet.gs.ui.rcp.main.views.bsimodelview" />
       			</or>
    		</with>
         </activeWhen> 
         <enabledWhen>
            	<with variable="selection">
	                <iterate operator="and">
	                    <instanceof value="sernet.gs.ui.rcp.main.common.model.CnATreeElement"/>
	                    <test property="sernet.verinice.iso27k.model.canContain"/>       
	                </iterate>
            	</with>
		 </enabledWhen>
      </handler>
