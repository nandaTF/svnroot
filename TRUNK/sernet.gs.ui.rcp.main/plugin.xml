         <action
               class="sernet.gs.ui.rcp.main.bsi.actions.AddAnwendungActionDelegate"
               enablesFor="+"
               icon="icons/application_osx.png"
               id="sernet.gs.ui.rcp.main.addanwendung"
               label="Neue Anwendung"
               menubarPath="additions"
               style="push"
               tooltip="Neue Anwendung hinzufügen"/>
      </objectContribution>
      <objectContribution
            adaptable="false"
            id="sernet.gs.ui.rcp.main.bsi.clientContrib"
            objectClass="sernet.gs.ui.rcp.main.bsi.model.ClientsKategorie">
         <action
               class="sernet.gs.ui.rcp.main.bsi.actions.AddClientActionDelegate"
               enablesFor="+"
               icon="icons/computer.png"
               id="sernet.gs.ui.rcp.main.addclient"
               label="Neuer Client"
               menubarPath="additions"
               style="push"
               tooltip="Neuen Client hinzufügen (PC, Laptop...)"/>
      </objectContribution>
      
       <objectContribution
            adaptable="false"
            id="sernet.gs.ui.rcp.main.bsi.sonstitContrib"
            objectClass="sernet.gs.ui.rcp.main.bsi.model.SonstigeITKategorie">
         <action
               class="sernet.gs.ui.rcp.main.bsi.actions.AddSonstItActionDelegate"
               enablesFor="+"
               icon="icons/sonstit.png"
               id="sernet.gs.ui.rcp.main.addsonstit"
               label="Neues IT-System"
               menubarPath="additions"
               style="push"
               tooltip="Neues sonstiges IT-System hinzufügen (PDA, Netzwerkdrucker...)"/>
      </objectContribution>
      
      <objectContribution
            adaptable="false"
            id="sernet.gs.ui.rcp.main.bsi.nkContrib"
            objectClass="sernet.gs.ui.rcp.main.bsi.model.NKKategorie">
         <action
               class="sernet.gs.ui.rcp.main.bsi.actions.AddNetzKomponenteActionDelegate"
               enablesFor="+"
               icon="icons/drive_network.png"
               id="sernet.gs.ui.rcp.main.addNk"
               label="Neues Netz / neue Verbindung"
               menubarPath="additions"
               style="push"
               tooltip="Netze / Verbindungen hinzufügen"/>
      </objectContribution>
      <objectContribution
            adaptable="false"
            id="sernet.gs.ui.rcp.main.bsi.tkContrib"
            objectClass="sernet.gs.ui.rcp.main.bsi.model.TKKategorie">
         <action
               class="sernet.gs.ui.rcp.main.bsi.actions.AddTelefonKomponenteActionDelegate"
               enablesFor="+"
               icon="icons/phone.png"
               id="sernet.gs.ui.rcp.main.addTk"
               label="Neue TK-Komponente"
               menubarPath="additions"
               style="push"
               tooltip="Neue Telekommunikations-Komponente"/>
      </objectContribution>
      <objectContribution
            adaptable="false"
            id="sernet.gs.ui.rcp.main.bsi.serverContrib"
            objectClass="sernet.gs.ui.rcp.main.bsi.model.ServerKategorie">
         <action
               class="sernet.gs.ui.rcp.main.bsi.actions.AddServerActionDelegate"
               enablesFor="+"
               icon="icons/server_database.png"
               id="sernet.gs.ui.rcp.main.addServer"
               label="Neuer Server"
               menubarPath="additions"
               style="push"
               tooltip="Neuen Server hinzufügen"/>
      </objectContribution>
      <objectContribution
            adaptable="false"
            id="sernet.gs.ui.rcp.main.bsi.raumContrib"
            objectClass="sernet.gs.ui.rcp.main.bsi.model.RaeumeKategorie">
         <action
               class="sernet.gs.ui.rcp.main.bsi.actions.AddRaumActionDelegate"
               enablesFor="+"
               icon="icons/door_open.png"
               id="sernet.gs.ui.rcp.main.addRaum"
               label="Neuer Raum"
               menubarPath="additions"
               style="push"
               tooltip="Neuen Raum hinzufügen"/>
      </objectContribution>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="sernet.gs.ui.rcp.main.bsi.editors.BSIElementEditor"
            default="false"
            icon="icons/24-tool-a.png"
            id="sernet.gs.ui.rcp.main.bsi.editors.bsielementeditor"
            name="BSI Objekteditor"/>
   </extension>
   <extension
