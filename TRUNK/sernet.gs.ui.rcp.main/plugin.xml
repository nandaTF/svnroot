            id="sernet.gs.ui.rcp.main.bsi.views.auditview"
            name="Prüfplan"/>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            name="Open View"
            categoryId="sernet.gs.ui.rcp.commands"
            id="sernet.gs.ui.rcp.main.open">
      </command>
      <command
            categoryId="sernet.gs.ui.rcp.commands"
            id="sernet.gs.ui.rcp.main.editmassnahme"
            name="sernet.gs.ui.rcp.main.editmassnahme"/>
      <category
            id="sernet.gs.ui.rcp.commands"
            name="verinice"/>
      <command
            categoryId="sernet.gs.ui.rcp.commands"
            description="Edit multiple elements all at once"
            id="sernet.gs.ui.rcp.main.actions.showbulkeditaction"
            name="Show Bulk Edit Dialog"/>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="org.eclipse.ui.file.exit"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="CTRL+SHIFT+X">
      </key>
      <key
            commandId="sernet.gs.ui.rcp.main.actions.showbulkeditaction"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="CTRL+SHIFT+B"/>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="sernet.gs.ui.rcp.main.application"
            name="verinice">
         <property
               name="aboutText"
               value="SerNet verinice&#x0A;&#x0A;(c) SerNet Service Network GmbH and others&#x0A;1996-2008&#x0A;&#x0A;&#x0A;Contact the author:&#x0A;verinice@sernet.de&#x0A;&#x0A;or&#x0A;&#x0A;Visit our homepage:&#x0A;http://www.verinice.org&#x0A;&#x0A;This program comes with ABSOLUTELY NO WARRANTY.&#x0A;This is free software, and you are welcome to redistribute it under certain conditions.&#x0A;Verinice is licensed to you under the conditions of the GPLv3.&#x0A;See the LICENSE.txt contained in the program folder or the verinice Plugin-Details for details."/>
         <property
               name="windowImages"
               value="icons/window16.gif,icons/window32.gif"/>
         <property
               name="aboutImage"
               value="icons/product_lg.gif"/>
         <property
               name="appName"
               value="verinice">
         </property>
      </product>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension targetID="org.eclipse.ui.resourcePerspective">
         <view
               id="sernet.gs.ui.rcp.main.views.BsiModelView"
               ratio="0.5"
               relationship="right"
               relative="org.eclipse.ui.views.TaskList"/>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            adaptable="false"
            id="sernet.gs.ui.rcp.main.elementcontrib"
            objectClass="sernet.gs.ui.rcp.main.common.model.CnATreeElement">
             <action
                   class="sernet.gs.ui.rcp.main.bsi.actions.DeleteActionDelegate"
                   enablesFor="+"
                   icon="icons/16-em-cross.png"
                   id="sernet.gs.ui.rcp.main.bsi.deleteaction"
                   label="Löschen"
                   menubarPath="content"
                   tooltip="BSI Objekt löschen">
                <enablement>
                   <and>
                   <or>
                   	<objectClass name="sernet.gs.ui.rcp.main.bsi.model.IBSIStrukturElement"/>
                   	<objectClass name="sernet.gs.ui.rcp.main.bsi.model.BausteinUmsetzung"/>
                   	<objectClass name="sernet.gs.ui.rcp.main.bsi.risikoanalyse.model.FinishedRiskAnalysis"/>
                   </or>
                   <not>
                      <objectClass name="sernet.gs.ui.rcp.main.bsi.model.ITVerbund"/>
                   </not>
                   </and>
                </enablement>
             </action>
             <action
                   class="sernet.gs.ui.rcp.main.bsi.actions.OpenAction"
                   enablesFor="+"
                   icon="icons/edtsrclkup_co.gif"
                   id="sernet.gs.ui.rcp.main.bsi.actions.openaction"
                   label="Öffnen"
                   menubarPath="content"
                   tooltip="Objekt öffnen">
                   <enablement>
                   <or>
                   	<objectClass name="sernet.gs.ui.rcp.main.bsi.model.IBSIStrukturElement"/>
                   	<objectClass name="sernet.gs.ui.rcp.main.bsi.model.BausteinUmsetzung"/>
                    <objectClass name="sernet.gs.ui.rcp.main.bsi.model.MassnahmenUmsetzung"/>
                   </or>
                </enablement>
             </action>
             <action
                   class="sernet.gs.ui.rcp.main.bsi.risikoanalyse.actions.NeueRisikoanalyseAction"
                   enablesFor="+"
                   icon="icons/16-message-warn.png"
                   id="sernet.gs.ui.rcp.main.neueRisikoanalyse"
                   label="Neue Risikoanalyse"
                   menubarPath="content"
                   tooltip="Risikoanalyse durchführen">
                <enablement>
                   <and>
                   		<objectClass name="sernet.gs.ui.rcp.main.bsi.model.IBSIStrukturElement"/>
                   <not>
                      <or>
                      		<objectClass name="sernet.gs.ui.rcp.main.bsi.model.ITVerbund"/>
                      		<objectClass name="sernet.gs.ui.rcp.main.bsi.model.Person"/>
                      </or>
                   </not>
                   </and>
                </enablement>
             </action>
      </objectContribution>
      
      <objectContribution
            adaptable="false"
            id="sernet.gs.ui.rcp.main.elementcontrib"
            objectClass="sernet.gs.ui.rcp.main.common.model.CnALink">
             <action
                   class="sernet.gs.ui.rcp.main.bsi.actions.DeleteLinkActionDelegate"
                   enablesFor="+"
                   icon="icons/link_delete.png"
                   id="sernet.gs.ui.rcp.main.bsi.deletelinkaction"
                   label="Verknüpfung entfernen"
                   menubarPath="content"
                   tooltip="Verknüpfung löschen">
              
             </action>
            
      </objectContribution>
      
          <objectContribution
            adaptable="false"
            id="sernet.gs.ui.rcp.main.bsi.categorycontrib"
            objectClass="sernet.gs.ui.rcp.main.common.model.CnATreeElement">
        <menu
               id="sernet.gs.ui.rcp.main.bsi.addsubmenu"
               label="Neu..."
               path="content">
            <groupMarker name="content"/>
            <separator name="additions"/>
         </menu>
       
      </objectContribution>
      <objectContribution
            adaptable="false"
            id="sernet.gs.ui.rcp.main.bsi.gebaeudecontrib"
            objectClass="sernet.gs.ui.rcp.main.bsi.model.GebaeudeKategorie">
        
         <action
               class="sernet.gs.ui.rcp.main.bsi.actions.AddGebaudeActionDelegate"
               enablesFor="+"
               icon="icons/building.png"
               id="sernet.gs.ui.rcp.main.addgebaeude"
               label="Neues Gebäude"
               menubarPath="additions"
               style="push"
               tooltip="Neues Gebäude dem IT Verbund hinzufügen"/>
        
      </objectContribution>
     
      <objectContribution
            adaptable="false"
            id="sernet.gs.ui.rcp.main.bsi.personcontrib"
