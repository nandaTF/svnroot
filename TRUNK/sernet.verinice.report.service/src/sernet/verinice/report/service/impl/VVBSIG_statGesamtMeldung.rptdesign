    <method name="initialize"><![CDATA[function formatShortDate(inputDate) {
	importPackage(Packages.java.text);
	importPackage(Packages.java.util);
	importPackage(Packages.java.lang);
	importPackage(Packages.sernet.verinice.report.service.impl);

	fDate = tryParseDate(inputDate);
	if(fDate != null){
		destinationFormat = new SimpleDateFormat("yyyy-MM-dd", Locale.GERMAN);
		return destinationFormat.format(fDate);
	} else {
		return "Datumsformat unbekannt";
	}


}

function formatLongDate(inputDate){
	importPackage(Packages.java.lang);
	importPackage(Packages.java.util);
	importPackage(Packages.java.text);
	importPackage(Packages.sernet.verinice.report.service.impl);
		
	fDate = tryParseDate(inputDate);
	if(fDate != null){
		destinationFormat = new SimpleDateFormat("dd. MMMMM yyyy", Locale.GERMAN);
		return destinationFormat.format(fDate);
	} else {
		return "Datumsformat unbekannt";
	}
}

function tryParseDate(inputDate){
	importPackage(Packages.java.lang);
	importPackage(Packages.java.util);
	importPackage(Packages.java.text);
	importPackage(Packages.sernet.verinice.report.service.impl);
	var	patterns = new Array("dd.MM.yy", "EE, dd.MM.yyyy", "dd.MM.yyyy", "dd.MM.yyyy HH:mm", "MMM dd, yyyy hh:mm aa", "MMM dd, yyyy HH:mm aa", "yyyy-MM-dd");
	var locales = new Array(Locale.GERMAN, Locale.ENGLISH);
	for(var i = 0; i < patterns.length; i++){
		for(var j = 0; j < locales.length; j++){
			try{
				formatter = new SimpleDateFormat(patterns[i], locales[j]);
				formatter.setLenient(true);
				return formatter.parse(inputDate);
			} catch(e){
				
			}
		}
	}	
	return null;
}]]></method>
