            <xml-property name="queryText"><![CDATA[import sernet.hui.common.*;
import sernet.gs.ui.rcp.main.service.crudcommands.*;


// set top level element for report:
root = helper.getRoot();
// try to get manual parameter if set:
myroot = _inpv.get("root_object");
if (myroot != null &amp;&amp; myroot != -1) {
	root = myroot;
}

results = new ArrayList(0);

toleratedC = Integer.parseInt(_inpv.get("toleratedC"));
toleratedI = Integer.parseInt(_inpv.get("toleratedI"));
toleratedA = Integer.parseInt(_inpv.get("toleratedA"));

c2 = new LoadReportAllRisksForScope(root, false, 
	LoadReportAllRisksForScope.RISK_WITH_IMPLEMENTED_CONTROLS);
c2 = helper.execute(c2);

elements = c2.getCountC();

cRedCount = 0;
cYellowCount = 0;
cGreenCount = 0;
iRedCount = 0;
iYellowCount = 0;
iGreenCount = 0;
aRedCount = 0;
aYellowCount = 0;
aGreenCount = 0;

for(int probability = 0; probability < elements.length; probability++){
	for(int impact = 0; impact < elements[probability].length; impact++){	

				cMinuend = 0;
				if(impact > 0){
					cMinuend = 2;
				} else {
					cMinuend = 3; // impact == 0
				}
		
				if((probability + impact) > toleratedC){
					cRedCount += elements[probability][impact];
				} else if((probability + impact) >= (toleratedC - cMinuend) &amp;&amp; (probability + impact) <= toleratedC){
					cYellowCount += elements[probability][impact];
				} else if((probability + impact +2) < toleratedC){
					cGreenCount += elements[probability][impact];
				}
			}
	}

elements = c2.getCountI();

for(int probability = 0; probability < elements.length; probability++){
	for(int impact = 0; impact < elements[probability].length; impact++){	
				if((probability + impact) > toleratedI){
					iRedCount += elements[probability][impact];
				} else if((probability + impact) >= (toleratedI - 4) &amp;&amp; (probability + impact) <= toleratedI){
					iYellowCount += elements[probability][impact];
				} else if((probability + impact +3) < toleratedI){
					iGreenCount += elements[probability][impact];
				}
			}
	}

elements = c2.getCountA();

for(int probability = 0; probability < elements.length; probability++){
	for(int impact = 0; impact < elements[probability].length; impact++){	
				if((probability + impact) > toleratedA){
					aRedCount += elements[probability][impact];
				} else if((probability + impact) >= (toleratedA - 3) &amp;&amp; (probability + impact) <= toleratedA){
					aYellowCount += elements[probability][impact];
				} else if((probability + impact +2) < toleratedA){
					aGreenCount += elements[probability][impact];
				}
			}
	}


result = new ArrayList(0);
result.add("Vertraulichkeit");
result.add(cRedCount);
result.add(cYellowCount);
result.add(cGreenCount);
result.add(0);
results.add(result);

result = new ArrayList(0);
result.add("Integrität");
result.add(iRedCount);
result.add(iYellowCount);
result.add(iGreenCount);
result.add(1);
results.add(result);

result = new ArrayList(0);
result.add("Verfügbarkeit");
result.add(aRedCount);
