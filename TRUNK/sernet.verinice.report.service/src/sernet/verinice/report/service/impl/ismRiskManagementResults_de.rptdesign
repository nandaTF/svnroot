            <xml-property name="queryText"><![CDATA[// DEPRECATED, VERY MEMORY INTENSIVE, USE scope_countrisks_aggregated_net_command INSTEAD OF THIS ONE (from verinice 1.6.3 on)
import sernet.hui.common.*;
import sernet.gs.ui.rcp.main.service.crudcommands.*;
import sernet.verinice.iso27k.service.IRiskAnalysisService;
import sernet.verinice.model.common.CnATreeElement;
import sernet.verinice.model.iso27k.Asset;
import sernet.verinice.model.iso27k.IncidentScenario;
import java.util.*;
import sernet.verinice.model.iso27k.Process;
import sernet.verinice.model.iso27k.AssetValueAdapter;
import sernet.verinice.iso27k.service.RiskAnalysisServiceImpl;
import sernet.gs.service.RetrieveInfo;
import sernet.verinice.service.commands.LoadElementByUuid;

// set top level element for report:
root = helper.getRoot();
// try to get manual parameter if set:
myroot = _inpv.get("root_object");
if (myroot != null &amp;&amp; myroot != -1) {
	root = myroot;
}

toleratedC = Integer.parseInt(_inpv.get("toleratedC"));
toleratedI = Integer.parseInt(_inpv.get("toleratedI"));
toleratedA = Integer.parseInt(_inpv.get("toleratedA"));

riskType = IRiskAnalysisService.RISK_WITH_IMPLEMENTED_CONTROLS;

results = new ArrayList(0);

cRedCount = 0;
cYellowCount = 0;
cGreenCount = 0;
iRedCount = 0;
iYellowCount = 0;
iGreenCount = 0;
aRedCount = 0;
aYellowCount = 0;
aGreenCount = 0;

//load processes
processLoader = new LoadReportElements(Process.TYPE_ID, root, true);
processes = helper.execute(processLoader).getElements();
assets = null;

for (CnATreeElement process : processes) {
		//load all assets for process
                assetLoader = new LoadReportLinkedElements(Asset.TYPE_ID, process.getDbId(), true, false);
		assets = helper.execute(assetLoader).getElements();
		for (CnATreeElement asset : assets) {
			// risk matrices
			  valueAdapter = new AssetValueAdapter(asset);
        		  raService = new RiskAnalysisServiceImpl();

			// reload asset
			assetReloader = new LoadElementByUuid(asset.getUuid(), new RetrieveInfo().setLinksDown(true).setLinksUp(true).setLinksDownProperties(true).setLinksUpProperties(true));
			asset = helper.execute(assetReloader).getElement();

        		impactC = valueAdapter.getVertraulichkeit();
        		impactI = valueAdapter.getIntegritaet();
		        impactA = valueAdapter.getVerfuegbarkeit();
       			Integer[] reducedImpact = raService.applyControlsToImpact(riskType, asset, impactC, impactI, impactA);
        		if (reducedImpact != null) {
            			impactC = reducedImpact[0];
            			impactI = reducedImpact[1];
            			impactA = reducedImpact[2];
        		}
			
			// load all scenarios for each asset
			scenarioLoader = new LoadReportLinkedElements(IncidentScenario.TYPE_ID, asset.getDbId());
			scenarios = helper.execute(scenarioLoader).getElements();
			isRedRisk = false;
			isYellowRisk = false;
			for (CnATreeElement scenario : scenarios) {
				probability = scenario.getNumericProperty(IRiskAnalysisService.PROP_SCENARIO_PROBABILITY_WITH_CONTROLS);
				isCRelevant = scenario.getEntity().getProperties("scenario_value_method_confidentiality").getProperty(0).getPropertyValue().equals("1");
       				isIRelevant = scenario.getEntity().getProperties("scenario_value_method_integrity").getProperty(0).getPropertyValue().equals("1");
       				isARelevant = scenario.getEntity().getProperties("scenario_value_method_availability").getProperty(0).getPropertyValue().equals("1");
			        // prob. / impact:
        			riskC = probability + valueAdapter.getVertraulichkeit();
        			riskI = probability + valueAdapter.getIntegritaet();
        			riskA = probability + valueAdapter.getVerfuegbarkeit();
				yellowCompareValueC = 3;
				yellowCompareValueI = 4;
				yellowCompareValueA = 3;

				cMinuend = 0;
				if(impactC > 0){
					cMinuend = 2;
				} else {
					cMinuend = 3;
				}
				
				cMinuend = 2; // seems to be the right choice in any case, please recheck
				if(isCRelevant){
					if((probability + impactC) > toleratedC){
						cRedCount++;
					//} else if((probability + impactC) >= (toleratedC - cMinuend) &amp;&amp; (probability + impactC) < toleratedC){ 
					//	cYellowCount++;
					} else if((probability + impactC +2) < toleratedC){
						cGreenCount++;
					} else {
						cYellowCount++;
					}
				}
				if(isIRelevant){ 
					if((probability + impactI) > toleratedI){
						iRedCount++;
					//} else if((probability + impactI) > (toleratedI - 4) &amp;&amp; (probability + impactI) < toleratedI){
					//	iYellowCount++;
					} else if((probability + impactI + 3) < toleratedI){
						iGreenCount++;
					} else {
						iYellowCount++;
					}
				}
				if(isARelevant){
					if((probability + impactA) > toleratedA){
						aRedCount++;
					//} else if((probability + impactA) > (toleratedA - 3) &amp;&amp; (probability + impactA) <= toleratedA){	
					//	aYellowCount++;
					} else if((probability + impactA +2) < toleratedA){
						aGreenCount++;
					} else {
						aYellowCount++;
					}
				}
       			 } // scenario iteration end
		} // asset iteration end
} // process iteration end

// prepare results
result = new ArrayList(0);
result.add("Vertraulichkeit");
result.add(cRedCount);
result.add(cYellowCount);
result.add(cGreenCount);
result.add(0);
results.add(result);

result = new ArrayList(0);
result.add("Integrität");
result.add(iRedCount);
result.add(iYellowCount);
result.add(iGreenCount);
result.add(1);
results.add(result);

result = new ArrayList(0);
result.add("Verfügbarkeit");
result.add(aRedCount);
result.add(aYellowCount);
result.add(aGreenCount);
result.add(2);
results.add(result);

return results;]]></xml-property>
