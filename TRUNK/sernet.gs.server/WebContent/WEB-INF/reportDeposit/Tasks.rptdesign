            <xml-property name="queryText"><![CDATA[import sernet.hui.common.*;
import sernet.gs.ui.rcp.main.service.crudcommands.*;
import sernet.verinice.report.service.commands.*;
import java.util.*;
import sernet.gs.ui.rcp.main.service.crudcommands.*;
import sernet.verinice.model.iso27k.*;
import sernet.gs.service.NumericStringComparator;
       

        // set top level element for report:
        root = helper.getRoot();
        // try to get manual parameter if set:
        myroot = _inpv.get("root_object");
        if (myroot != null &amp;&amp; myroot != -1) {
            root = myroot;
        }


        // load all orgs:
            c2 = new LoadReportParent(root);
            iso27Root = helper.execute(c2).getElements().get(0).getDbId();

            c2 = new LoadReportElements("org", iso27Root);
            elements = helper.execute(c2).getElements();
            List allOrgIDs = new ArrayList();
            List allOrgNames = new ArrayList();

            for (Organization org: elements) {
                allOrgIDs.add(org.getDbId());
                allOrgNames.add(org.getTitle());
            }


            cols = helper.getAllPropertyTypes("audit");

            List allISAs = new ArrayList();
            int chapterNameIdx =0;

            // load ISAs for each organization:
            for (int i=0; i < allOrgIDs.size(); i++) {
                List orgISADbIds = new ArrayList();
                List orgISAs = new ArrayList();
                c2 = new LoadReportElements("audit", (Integer) allOrgIDs.get(i));
                elements = helper.execute(c2).getElements();

                for (Audit isa: elements) {
                    orgISADbIds.add(isa.getDbId());
                }
                orgISAs = helper.map(
                        elements,
                        cols,
                        true
                        );

                // add orgName column to each row:
                    for (List row : orgISAs) {
                        row.add(allOrgNames.get(i));
                    }

                    // add total result figure to each row:
                    int j=0;
                    for (List row: orgISAs) {
                        isaResult = helper.execute(
                                new sernet.verinice.samt.service.TotalSecurityFigureCommand(
                                        orgISADbIds.get(j)
                                        )
                                ).getResult();
                        row.add(isaResult);
                        j++;
                    }


                    // load chapters:
                        List orgChapters = new ArrayList();
                    int k=0;

                    for (Object obj : orgISADbIds) {
                        Integer isaDbID = (Integer) obj;
                        c = helper.execute(new LoadSamtComplianceRadarChartData(isaDbID));
                        List chapterRows = c.getResult();
                        for (List chapterRow : chapterRows) {
                            // build row with isaRow + chapter and maturity columns:
                            List newRow = new ArrayList();
                            newRow.addAll(orgISAs.get(k));
                            chapterNameIdx = newRow.size();
                            newRow.add(chapterRow.get(0));
                            newRow.add(chapterRow.get(1));
                            orgChapters.add(newRow);
                        }
                        k++;
                    }


                    allISAs.addAll(orgChapters);


            }

            int idx = chapterNameIdx;

            // pre-sort list by chapter name:
            Collections.sort(allISAs, new Comparator() {
                public int compare(List row1, List row2) {
                    NumericStringComparator comp = new NumericStringComparator();
                    if (row1 == null || row2 == null)
                        return 0;
                    if (row1.get(idx) == null || row2.get(idx) == null)
                        return 0;
                    return comp.compare(row1.get(idx), row2.get(idx));
                }
            });

            return allISAs;




    ]]></xml-property>
