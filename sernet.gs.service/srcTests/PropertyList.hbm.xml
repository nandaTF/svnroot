<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="sernet.hui.common.connect">
	<class name="PropertyList" table="propertylist">
	
		<id name="dbId" column="dbId" type="java.lang.Integer" 
			unsaved-value="null">
			<generator class="native" />
		</id>
		
		<list name="properties" cascade="all-delete-orphan" lazy="false">
			<key column="properties_id" />
			<index 
				column="properties_idx" />
			<one-to-many class="Property" />
		</list>

	</class>
</hibernate-mapping>
