<form id="quick-search" action="/repository/app/search" method="get">
	<fieldset>
		<input type="text" name="query" />
		<input type="submit" name="submit" value="Search" />
	</fieldset>
	<script type="text/javascript">
		dojo.addOnLoad(function(){
			var defaultQuery = 'Quick Search';
			var form = dojo.byId("quick-search");
			var submit = form.elements['submit'];
			var input =  form.elements['query'];
			submit.parentNode.removeChild(submit);
			input.value = defaultQuery;
			input.onfocus = function() { if (this.value == defaultQuery) this.value = ''; };
			input.onblur = function() { if (this.value == '') this.value = defaultQuery; };
		});
	</script>
</form>

