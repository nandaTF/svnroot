<script type="text/javascript">
	dojo.query('.spring-titlePane-open > h2').forEach(function(titleElement){
		Spring.addDecoration(new Spring.ElementDecoration({
			elementId : titleElement.parentNode.id,
			widgetType : 'dijit.TitlePane',
			widgetAttrs : { title : titleElement.innerHTML, open : true }}));
	}).style('display','none');
	
	dojo.query('.spring-titlePane > h2').forEach(function(titleElement){
		Spring.addDecoration(new Spring.ElementDecoration({
			elementId : titleElement.parentNode.id,
			widgetType : 'dijit.TitlePane',
			widgetAttrs : { title : titleElement.innerHTML, open : false }}));
	}).style('display','none');
	
	dojo.query('#exports a').forEach(function(element){
		Spring.addDecoration(new Spring.AjaxEventDecoration({
			elementId : element.id,
			event : 'onclick',
			params : {fragments : 'export-fragment'}
		}));
	});
	
	dojo.query('.spring-titlePane table.bordered-table, .spring-titlePane-open table.bordered-table').removeClass('bordered-table');
</script>

